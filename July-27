  int minOperations(int[] arr, int n, int k) {
       PriorityQueue<Integer> pq = new PriorityQueue<>();
       int res=0;
       for(int i=0;i<n;i++)
       {
           pq.add(arr[i]);
       }
       while(pq.peek()<k)
       {
           int a=pq.remove();
           a=a+pq.remove();
           pq.add(a);
           res++;
       }
       return res;
   }
