 long long int minSum(int arr[], int n)
   {
       sort(arr,arr+n);
       long long int  first=0;
       long long int second=0;
       for(int i=0;i<n;i++){
           if(i%2==0)
           {
               first=first*10+arr[i];
               
               
           }
           else
           {
               second=second*10+arr[i];
           }
       }
       
       return first+second;
       // Your code goes here
   }
